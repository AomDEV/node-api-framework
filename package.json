{
  "name": "api-framework",
  "version": "1.0.0",
  "description": "NodeJS (Typescript) API Framework",
  "main": "index.js",
  "scripts": {
    "prepare": "ts-patch install -s",
    "dev": "nodemon",
    "test": "jest",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "build": "tsc -b .",
    "prod": "ts-node dist/app.js"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "src"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "keywords": [
    "nodejs",
    "api",
    "framework",
    "ts"
  ],
  "author": "@AOM",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/express-jwt": "^0.0.42",
    "@types/jest": "^27.0.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.6.1",
    "@types/redis": "^2.8.32",
    "@types/supertest": "^2.0.11",
    "jest": "^27.2.1",
    "nodemon": "^2.0.12",
    "prisma": "^2.29.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.0",
    "ts-patch": "^1.4.2",
    "tslint": "^5.12.1",
    "typescript": "^4.4.3",
    "typescript-transform-paths": "^3.3.0"
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@prisma/client": "^3.0.2",
    "babel-jest": "^27.2.1",
    "body-parser": "^1.18.3",
    "cli-table": "^0.3.6",
    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "express-jwt": "^6.1.0",
    "express-validator": "^6.12.1",
    "redis": "^3.1.2",
    "request-to-curl": "^0.1.6",
    "tsconfig-paths": "^3.10.1",
    "uuid": "^8.3.2"
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ]
}
